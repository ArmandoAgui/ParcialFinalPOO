<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="507.0" prefWidth="590.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.parcialfinalpoo.ClienteController">
    <children>
        <GridPane fx:id="gridPane" alignment="TOP_CENTER" hgap="10.0" layoutX="10.0" layoutY="10.0" prefHeight="476.0" prefWidth="580.0" vgap="10.0">

            <HBox alignment="CENTER" spacing="10.0" GridPane.columnIndex="0" GridPane.rowIndex="5">
                <Button fx:id="btnAgregar" text="Agregar" />
                <Button fx:id="btnActualizar" disable="true" text="Actualizar" />
                <Button fx:id="btnEliminar" disable="true" text="Eliminar" />
                <Button fx:id="btnLimpiar" text="Limpiar" />
            </HBox>
            <HBox alignment="CENTER" spacing="10.0" GridPane.columnIndex="0" GridPane.rowIndex="4">
                <TextField fx:id="txtBuscarDui" prefHeight="26.0" prefWidth="207.0" promptText="Buscar por DUI" />
                <Button fx:id="btnBuscar" text="Buscar" />
            </HBox>
            <TableView fx:id="tblClientes" prefHeight="227.0" prefWidth="570.0" GridPane.columnIndex="0" GridPane.rowIndex="6">
                <columns>
                    <TableColumn fx:id="colId" prefWidth="50.0" text="ID" />
                    <TableColumn fx:id="colNombreCompleto" prefWidth="150.0" text="Nombre Completo" />
                    <TableColumn fx:id="colDireccion" prefWidth="130.0" text="Dirección" />
                    <TableColumn fx:id="colTelefono" prefWidth="120.0" text="Teléfono" />
                    <TableColumn fx:id="colDui" prefWidth="120.0" text="Dui" />
                </columns>
            </TableView>
         <HBox alignment="CENTER_LEFT" prefHeight="33.0" prefWidth="500.0" GridPane.rowIndex="2">
            <children>
               <Label text="Telefono" />
                  <TextField fx:id="txtTelefono" prefHeight="26.0" prefWidth="388.0">
                  <HBox.margin>
                     <Insets left="58.0" />
                  </HBox.margin>
               </TextField>
            </children>
         </HBox>
         <HBox alignment="CENTER_LEFT" prefHeight="41.0" prefWidth="570.0">
            <children>
               <Label text="Nombre Completo" />
                  <TextField fx:id="txtNombreCompleto" prefHeight="26.0" prefWidth="385.0">
                  <HBox.margin>
                     <Insets left="5.0" />
                  </HBox.margin>
               </TextField>
            </children>
         </HBox>
         <HBox alignment="CENTER_LEFT" prefHeight="39.0" prefWidth="570.0" GridPane.rowIndex="1">
            <children>
               <Label text="Direccion" />
                  <TextField fx:id="txtDireccion" prefHeight="26.0" prefWidth="385.0">
                  <HBox.margin>
                     <Insets left="55.0" />
                  </HBox.margin>
               </TextField>
            </children>
         </HBox>
            <HBox alignment="CENTER_LEFT" prefHeight="41.0" prefWidth="570.0" GridPane.rowIndex="3">
                <children>
                    <Label text="Dui" />
                    <TextField fx:id="txtDui" prefHeight="26.0" prefWidth="385.0">
                        <HBox.margin>
                            <Insets left="85.0" />
                        </HBox.margin>
                    </TextField>
                </children>
            </HBox>
         <columnConstraints>
            <ColumnConstraints />
            <ColumnConstraints />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints />
             <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
            <RowConstraints />
             <RowConstraints />
         </rowConstraints>
        </GridPane>
    </children>
</AnchorPane>
